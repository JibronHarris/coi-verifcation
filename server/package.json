{
  "name": "coi-verification-be",
  "version": "1.0.0",
  "description": "Backend for coi-verification app",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only src/index.ts",
    "prisma:generate": "dotenv -e .env.dev -- prisma generate",
    "prisma:migrate": "dotenv -e .env.dev -- prisma migrate dev",
    "prisma:studio": "dotenv -e .env.dev -- prisma studio"
  },
  "author": "Jibron Harris",
  "license": "ISC",
  "dependencies": {
    "@auth/core": "^0.37.2",
    "@auth/prisma-adapter": "^2.6.0",
    "@prisma/client": "^5.19.1",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.7.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "dotenv-cli": "^11.0.0",
    "prisma": "^5.19.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
